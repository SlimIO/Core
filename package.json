{
    "name": "@slimio/core",
    "version": "0.6.0",
    "description": "Slim.IO Core",
    "main": "index.js",
    "scripts": {
        "prepublish": "pkg-ok",
        "test": "node test/japaRunner.js",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
        "coverage": "nyc npm test",
        "report": "nyc report --reporter=html"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SlimIO/Core.git"
    },
    "author": "SlimIO",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SlimIO/Core/issues"
    },
    "homepage": "https://github.com/SlimIO/Core#readme",
    "nyc": {
        "check-coverage": false,
        "per-file": false,
        "lines": 98,
        "statements": 98,
        "functions": 98,
        "branches": 90
    },
    "husky": {
        "hooks": {
            "pre-push": "npm test",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "devDependencies": {
        "@commitlint/cli": "^7.2.1",
        "@commitlint/config-conventional": "^7.1.2",
        "@escommunity/minami": "^1.0.0",
        "@slimio/addon-factory": "^0.3.0",
        "@slimio/eslint-config": "^1.1.8",
        "@types/es6-shim": "^0.31.39",
        "@types/node": "^10.12.5",
        "@types/rimraf": "^2.0.2",
        "@types/uuid": "^3.4.4",
        "@types/zen-observable": "^0.8.0",
        "eslint": "^5.9.0",
        "husky": "^1.1.3",
        "japa": "^2.0.6",
        "nyc": "^13.1.0",
        "pkg-ok": "^2.3.1",
        "rimraf": "^2.6.2"
    },
    "dependencies": {
        "@slimio/addon": "^0.13.2",
        "@slimio/config": "^0.7.0",
        "@slimio/is": "^1.2.0",
        "@slimio/safe-emitter": "^1.0.1",
        "@slimio/scheduler": "^0.5.0",
        "@slimio/utils": "^0.5.0",
        "make-promises-safe": "^3.0.0",
        "uuid": "^3.3.2"
    }
}

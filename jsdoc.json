{
    "opts": {
        "encoding": "utf8",
        "destination": "./jsdoc/",
        "recurse": true,
        "template": "node_modules/@escommunity/minami"
    },
    "source": {
        "include": [
			"index.js",
			"./src/core.class.js",
			"./src/forked.container.js",
			"./src/parallelAddon.class.js",
			"./src/utils.js"
        ],
        "exclude": [
            "node_modules",
            "client"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    }
}
